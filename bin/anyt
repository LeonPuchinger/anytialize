#!/usr/bin/env python
import sys
import anytialize as anyt

if __name__ == '__main__':
    args = sys.argv[1:]
    if len(args) < 1:
        print("Invalid syntax: Enter at least one argument")
        sys.exit(1)
    try:
        anyt.execute(args[0], args[1:])
    except anyt.AnytException as e:
        print(e)
